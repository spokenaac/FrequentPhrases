  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.5.4"
